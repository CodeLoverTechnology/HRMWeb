
@{
    ViewBag.Title = "TimeSheet_Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div id="TimeSheetLeave" class="border shadow mt-4 p-0">
            <h3 class="bg-primary text-white p-1 pl-3 mt-0">Time Sheet Index</h3>
            <div class="p-3">
                <div class="form-group">
                    <label id="lblTimeSheetID" class="control-label">Time Sheet ID</label>
                    <input type="text" id="txtTimeSheetID" name="txtTimeSheetID" class="form-control-Label" readonly="readonly" />
                    <div class="clearfix"></div>
                </div>
                <div class="form-group">
                    <label id="lblProject" class="control-label">Project</label>
                    <input type="text" id="txtProject" name="txtProject" class="form-control-Label" readonly="readonly" />
                    <div class="clearfix"></div>
                </div>
                <div class="form-group">
                    <label id="lblTypeOfWork" class="control-label">Type Of Work</label>
                    <input type="text" id="txtTypeOfWork" name="txtTypeOfWork" class="form-control-Label" readonly="readonly" />
                    <div class="clearfix"></div>
                </div>
                <div class="form-group">
                    <label id="lblDate" class="control-label">Date</label>
                    <input type="text" id="txtDate" name="txtDate" class="form-control-Label" readonly="readonly" />
                    <div class="clearfix"></div>
                </div>
                <div class="form-group">
                    <label id="lblHourse" class="control-label">Hourse</label>
                    <input type="text" id="txtHourse" name="txtHourse" class="form-control-Label" readonly="readonly" />
                    <div class="clearfix"></div>
                </div>
                <div class="form-group">
                    <label id="lblWorkDescription" class="control-label">Work Description</label>
                    <input type="text" id="txtWorkDescription" name="txtWorkDescription" class="form-control-Label" readonly="readonly" />
                    <div class="clearfix"></div>
                </div>
                <div class="form-group">
                    <label id="lblApprovedBy" class="control-label">Approved By</label>
                    <input type="text" id="txtApprovedBy" name="txtApprovedBy" class="form-control-Label" readonly="readonly" />
                    <div class="clearfix"></div>
                </div>
                <div class="form-group">
                    <div class="text-center mt-5">
                        <a href="#" class="btn btn-primary">View</a>
                        <a href="#" class="btn btn-success">Edit</a>
                        <a href="#" class="btn btn-danger">Delete</a>
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

